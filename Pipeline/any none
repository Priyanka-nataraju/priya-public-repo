pipeline {
    agent none
    stages{
        stage('BUILD'){
            agent {
                label 'slave1'
            }
            steps{
                echo "building in salve1"
            }
        }
        
        stage('TEST'){
            agent {
                label 'salve2'
            }
            steps{
                    echo " testing in slave2"
                }
        }
    }
}
